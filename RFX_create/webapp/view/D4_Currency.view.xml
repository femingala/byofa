<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="generated.app.controller.D4_Currency" xmlns:m="sap.m">
	<m:Dialog id="sap_m_Dialog_0" title="Currency" horizontalScrolling="true" showHeader="true" state="None" stretch="false" type="Standard"
		verticalScrolling="true">
		<m:content>
			<m:List items="{path: '/currencyHelpSet',sorter: 'Currency'}" growing="true" growingThreshold="10">
				<m:items>
					<m:StandardListItem id="selectedCurrency" title="{Currency}" description="" info="{CurrencyText}"
						type="Active" selected="false" press="onSelect"/>
				</m:items>
			</m:List>
		</m:content>
		<m:beginButton/>
		<m:buttons>
			<m:Button id="sap_m_Dialog_0-buttons-sap_m_Button-2" iconDensityAware="false" text="Cancel" type="Default" iconFirst="true" width="auto"
				enabled="true" visible="true" press="onClose"/>
		</m:buttons>
		<m:customHeader/>
		<m:endButton/>
		<m:subHeader/>
	</m:Dialog>
</mvc:View>